[ext_resource type="Script" uid="uid://bpqxvagkkj531" path="res://Util/DialogueLine.gd" id="1_ukc0c"]
[ext_resource type="Resource" uid="uid://0lyyamy2wpem" path="res://Assets/Speakers/SpeakerBrennan.tres" id="2_rq51q"]
[ext_resource type="Resource" uid="uid://dsofx6suvp8el" path="res://Assets/Speakers/SpeakerPenny.tres" id="3_ywrtf"]
[ext_resource type="Script" uid="uid://cbv2lkkta35mj" path="res://Util/DialogueScene.gd" id="4_ph552"]

[sub_resource type="Resource" id="Resource_tebp8"]
script = ExtResource("1_ukc0c")
speaker = ExtResource("2_rq51q")
line = "Mrs. Whayne... could you answer me something real quick?"
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_ukc0c"]
script = ExtResource("1_ukc0c")
speaker = ExtResource("3_ywrtf")
line = "Hit me."
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_rq51q"]
script = ExtResource("1_ukc0c")
speaker = ExtResource("2_rq51q")
line = "So, you said you were the only one working in the restaurant during closing time, right?"
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_ywrtf"]
script = ExtResource("1_ukc0c")
speaker = ExtResource("3_ywrtf")
line = "M-hm."
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_ph552"]
script = ExtResource("1_ukc0c")
speaker = ExtResource("2_rq51q")
line = "And you were also the only employee when the murder happened?"
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_5lbgs"]
script = ExtResource("1_ukc0c")
speaker = ExtResource("3_ywrtf")
line = "That's what I said, yes."
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_loqap"]
script = ExtResource("1_ukc0c")
speaker = ExtResource("2_rq51q")
line = "You see, the knife used in the crime was branded with the restaurant's name: \"The Grand Apricot\". That means, it must have come from here."
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_aycwd"]
script = ExtResource("1_ukc0c")
speaker = ExtResource("3_ywrtf")
line = "I mean, it's a restaurant, there's knives everywhere."
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_4qth4"]
script = ExtResource("1_ukc0c")
speaker = ExtResource("2_rq51q")
line = "Yes, but, as I understand, these knives are only for use within the kitchen; as in, no patrons should have access to it."
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_4v82e"]
script = ExtResource("1_ukc0c")
speaker = ExtResource("2_rq51q")
line = "Have you used a knife like this during your shift tonight Mrs. Whayne?"
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_60d8l"]
script = ExtResource("1_ukc0c")
speaker = ExtResource("3_ywrtf")
line = "I guess it's possible... We only have a couple of these knives, [color=yellow]I may have even touched the one used in the crime, but...[/color]"
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_45aoh"]
script = ExtResource("1_ukc0c")
speaker = ExtResource("3_ywrtf")
line = "Please hear me out, [color=yellow]I was alone in the kitchen the whole time,[/color] literally anyone could have gone in and grabbed a knife while I wasn't looking!"
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_ev52g"]
script = ExtResource("1_ukc0c")
speaker = ExtResource("3_ywrtf")
line = "[color=yellow]We have one in the bar near the entrance[/color] as well, they're not exactly hard to get."
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_quu1i"]
script = ExtResource("1_ukc0c")
speaker = ExtResource("3_ywrtf")
line = "Besides, if some bad guy came by and tried to grab a knife it’s not like I’d be able to stop them… [color=yellow]I'm not exactly weak but I've seen better days.[/color]"
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_xukgq"]
script = ExtResource("1_ukc0c")
speaker = ExtResource("3_ywrtf")
line = "Please, I swear... [color=yellow]I didn’t get anywhere near that pineapple.[/color]"
metadata/_custom_type_script = "uid://bpqxvagkkj531"

