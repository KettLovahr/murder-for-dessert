[sub_resource type="Resource" id="Resource_o4see"]
script = ExtResource("1_f8buk")
speaker = ExtResource("2_oywe6")
line = "Like I said, I'm investigating a crime - a murder! Did you see anything that could help me figure out who did it?"
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_s7dgp"]
script = ExtResource("1_f8buk")
speaker = ExtResource("3_1jyln")
line = "I saw the fly lady leave, I saw her come back, I heard the screams of horror, [color=yellow]and even that grating noise that shook the floor![/color]"
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_m3lbt"]
