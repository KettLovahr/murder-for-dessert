[sub_resource type="Resource" id="Resource_f8buk"]
script = ExtResource("1_f8buk")
speaker = ExtResource("2_oywe6")
line = "Hey little Billy! I talked to the big nose man, and he told me he did cry! He cried all night, just like you said!"
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_oywe6"]
script = ExtResource("1_f8buk")
speaker = ExtResource("3_1jyln")
line = "Why, did you doubt me? [color=yellow]You think I'd lie to a cop?[/color] Also don't call me Billy!"
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_1jyln"]
script = ExtResource("1_f8buk")
speaker = ExtResource("2_oywe6")
line = "I'm sorry! Mr. Madison..."
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_xs3ar"]
script = ExtResource("1_f8buk")
speaker = ExtResource("3_1jyln")
line = "What? Mr. Madison is my dad! He's not here."
metadata/_custom_type_script = "uid://bpqxvagkkj531"

