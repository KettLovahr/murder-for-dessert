[sub_resource type="Resource" id="Resource_sa82j"]
script = ExtResource("1_jofa1")
speaker = ExtResource("2_ic6o4")
line = "Excuse me Richard, I got one more thing for you."
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_jofa1"]
script = ExtResource("1_jofa1")
speaker = ExtResource("3_iedgh")
line = "It's Mr. Richardson to you."
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_ic6o4"]
script = ExtResource("1_jofa1")
speaker = ExtResource("2_ic6o4")
line = "Whatever. You told me that you weren't there at the time of the murder, didn't you?"
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_iedgh"]
script = ExtResource("1_jofa1")
speaker = ExtResource("3_iedgh")
line = "Yes, if you'd remember, I told you I was away, [color=yellow]answering an important phonecall.[/color]"
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_iydlh"]
script = ExtResource("1_jofa1")
speaker = ExtResource("2_ic6o4")
line = "Then why did Ms. Flowers over there say that you were already in the room when she arrived?"
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_701qc"]
script = ExtResource("1_jofa1")
speaker = ExtResource("2_ic6o4")
line = "I'd assumed she was the first one who saw the dead body. But now I think you might have seen it before her."
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_q3ob2"]
script = ExtResource("1_jofa1")
speaker = ExtResource("3_iedgh")
line = "Oh, I uhm..."
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_1m83o"]
script = ExtResource("1_jofa1")
speaker = ExtResource("3_iedgh")
line = "I was... [color=yellow]sat behind a pillar.[/color]"
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_m8a6t"]
script = ExtResource("1_jofa1")
speaker = ExtResource("3_iedgh")
line = "Yeah, I was behind a pillar, [color=yellow]couldn't see Ms. Bezerra's table at all![/color]"
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_j17tf"]
script = ExtResource("1_jofa1")
speaker = ExtResource("3_iedgh")
line = "Terrible blunder on my part."
metadata/_custom_type_script = "uid://bpqxvagkkj531"

[sub_resource type="Resource" id="Resource_p6hmw"]
